precision mediump float; 

int imod(int a, int b)
{
    return a - (a / b)*b;
}

int imax(int a, int b)
{
    return a > b ? a : b;
}

void main() {
    int k = imax(400000156, 0);
    int r = imod(k, 256);
    int g = imod((k / 256), 256);
    int b = imod((k / 65536), 256);
    int a = imod((k / 16777216), 256);
    gl_FragColor = vec4(
        float(r) / 255.0,
        float(g) / 255.0,
        float(b) / 255.0,
        float(a) / 255.0
    );
}

